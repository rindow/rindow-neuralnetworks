    "suggest": {
        "rindow/rindow-math-plot": "when visualize machine learning results",
        "ext-pdo_sqlite": "for save model",
        "ext-rindow_openblas": "0.2.4 strongly recommended for speed",
        "ext-rindow_opencl": "0.1.3 GPU/OpenCL support",
        "ext-rindow_clblast": "0.1.2 GPU/OpenCL support"
    },
    "autoload": {
        "psr-4": {
            "Rindow\\NeuralNetworks\\": "src/"
        }
    }
